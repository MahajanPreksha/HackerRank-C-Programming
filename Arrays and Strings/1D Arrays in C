#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, sum = 0;
    int arr[1000];
    int i;
    scanf("%d", &n);
    for(int i = 0; i<n; i++){
        scanf("%d ", &arr[i]);
    }
    for (i=0; i<n; ++i){
        sum +=arr[i];
    }
    printf("%d", sum);
    return 0;
}
